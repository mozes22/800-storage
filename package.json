{
  "name": "800-storage",
  "version": "0.0.0",
  "private": true,
  "author": "800 Storage Dev Team",
  "license": "UNLICENSED",
  "description": "Angular frontend demo with users list, search, and details view.",
  "homepage": "https://800storagedubai.com/",
  "engines": {
    "node": "22.22.0"
  },
  "engineStrict": true,
  "packageManager": "npm@10.9.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean-install": "npm cache clean --force && npm ci",
    "serve:ssr:800-storage": "node dist/800-storage/server/server.mjs",
    "lint": "ng lint",
    "lint:check": "npm exec eslint \"src/**/*.{js,ts}\" -- --quiet --cache --cache-location ./.lint-cache/eslint/",
    "lint:format": "npm exec eslint \"src/**/*.{js,ts}\" -- --fix --quiet --cache --cache-location ./.lint-cache/eslint/",
    "lint:style:check": "npm exec stylelint \"**/*.{css,scss}\" -- --quiet --cache --cache-location ./.lint-cache/stylelint/",
    "lint:style:format": "npm exec stylelint \"**/*.{css,scss}\" -- --fix --quiet --cache --cache-location ./.lint-cache/stylelint/",
    "format:check": "npm exec prettier --check \"src/**/*.{html,scss,css,ts,js,json,md}\" --ignore-path .prettierignore",
    "format": "npm exec prettier --write \"src/**/*.{html,scss,css,ts,js,json,md}\" --ignore-path .prettierignore",
    "clear-lint": "rm -fr .lint-cache/",
    "pkg:check": "npm-check-updates --packageManager npm",
    "pkg:update": "npm-check-updates --packageManager npm -u",
    "pkg:outdated": "npm outdated || true",
    "gen:tree": "node ./tools/misc-scripts/dir-tree.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.19",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.0",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-eslint": "21.0.1",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.3.1",
    "postcss": "^8.5.6",
    "prettier": "3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-tailwindcss": "^1.0.1",
    "tailwindcss": "^4.1.18",
    "tree-cli": "^0.6.7",
    "typescript": "^5.7.2",
    "typescript-eslint": "8.46.4"
  }
}
